// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  @@map("users")

  id String @id
  login String
  password String
  version Int @default(1)
  createdAt BigInt
  updatedAt BigInt
}

model Album {
  @@map("albums")

  id String @id
  name String
  year Int
  artistId String?
}
